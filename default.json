{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:recommended",
    "group:allNonMajor",
    "npm:unpublishSafe",
    "schedule:automergeWeekends",
    "schedule:weekends",
    ":automergeStableNonMajor",
    ":preserveSemverRanges"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["^@dcloudio/"],
      "groupName": "@dcloudio"
    }
  ]
}
